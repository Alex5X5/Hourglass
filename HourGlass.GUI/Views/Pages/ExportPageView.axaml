<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="clr-namespace:Hourglass.GUI.ViewModels.Pages"
			 x:Class="Hourglass.GUI.Views.Pages.ExportPageView"
			 x:DataType="vm:ExportPageViewModel"
			 x:CompileBindings="True">
	<Design.DataContext>
		<vm:ExportPageViewModel />
	</Design.DataContext>
	<Grid RowDefinitions="5*, 35*, *, 4*, *" ColumnDefinitions="*, 10*, *">
		<ScrollViewer HorizontalScrollBarVisibility="Disabled"
					  VerticalScrollBarVisibility="Visible"
					  Grid.Column="1"
					  Grid.Row="1">
			<Viewbox Stretch="Uniform">				
				<Grid Width="17" Height="12" Background="Beige">
					
				</Grid>
			</Viewbox>
		</ScrollViewer>
		<Grid Grid.Row="3"
			  Grid.Column="1"
			  ColumnDefinitions="3*, 4*, 2*, 4*, 3*">
			<Button Grid.Column="1" Command="{ Binding ImportCommand }" >
				<TextBlock Classes="ButtonContent" Text="Import" />
			</Button>
			<Button Grid.Column="3" Command="{ Binding ExportCommand }" >
				<TextBlock Classes="ButtonContent" Text="Export" />
			</Button>
		</Grid>
	</Grid>
</UserControl>
