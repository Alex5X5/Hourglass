<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="clr-namespace:Hourglass.GUI.ViewModels.Pages"
			 x:Class="Hourglass.GUI.Views.Pages.ExportPageView"
			 x:DataType="vm:ExportPageViewModel"
			 x:CompileBindings="True">
	<Design.DataContext>
		<vm:ExportPageViewModel />
	</Design.DataContext>
	<Grid RowDefinitions="*, 5*, *" ColumnDefinitions="*, 10*, *">
		<ScrollViewer HorizontalScrollBarVisibility="Disabled"
					  VerticalScrollBarVisibility="Visible"
					  Grid.Column="1"
					  Grid.Row="1">
			<Viewbox Stretch="Uniform">				
				<Grid Width="17" Height="12" Background="Beige">
					
				</Grid>
			</Viewbox>
		</ScrollViewer>
	</Grid>
</UserControl>
